{
  "author": "Arian Acosta (https://twitter.com/arian3k)",
  "bin": "bin/install-boilerplate.js",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fs-extra": "^8.1.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  },
  "description": "Automatically install and maintain your code style with linters and formatters for JavaScript, TypeScript and React.",
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "commitizen": "^4.0.3",
    "husky": "^4.0.0-beta.5",
    "lint-staged": "^9.4.3",
    "semantic-release": "^15.13.30",
    "semantic-release-npm-github-publish": "^1.4.0"
  },
  "homepage": "https://github.com/arianacosta/poetic",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "poetic",
    "style",
    "linter",
    "formatter",
    "eslint",
    "tslint",
    "javascript",
    "typescript",
    "prettier",
    "airbnb"
  ],
  "license": "MIT",
  "lint-staged": {
    "extends": []
  },
  "main": "config/index.js",
  "name": "poetic",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arianacosta/poetic.git"
  },
  "scripts": {
    "cz": "git-cz",
    "develop": "node scripts/develop.js",
    "upgrade:dependencies": "npx npm-check-updates -u"
  },
  "version": "1.1.0"
}
